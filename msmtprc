account wright.grayson
host smtp.gmail.com
port 587
protocol smtp
auth on
from wright.grayson@gmail.com
user wright.grayson@gmail.com
passwordeval /usr/local/bin/lpass show "Email/google.com" | ag "^Password: " | sed "s/^Password: //"
tls on
tls_trust_file ~/.mutt/Equifax_Secure_CA.cert

account default : wright.grayson
